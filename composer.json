{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "afatmustafa/blade-hugeicons": "^1.0",
        "andreiio/blade-iconoir": "^4.7",
        "codeat3/blade-carbon-icons": "^2.36",
        "codeat3/blade-fluentui-system-icons": "^1.34",
        "codeat3/blade-forkawesome": "^1.11",
        "codeat3/blade-game-icons": "^1.5",
        "codeat3/blade-google-material-design-icons": "^1.20",
        "codeat3/blade-iconpark": "^1.7",
        "codeat3/blade-line-awesome-icons": "^1.5",
        "codeat3/blade-majestic-icons": "^2.2",
        "codeat3/blade-maki-icons": "^1.9",
        "codeat3/blade-radix-icons": "^1.8",
        "codeat3/blade-simple-line-icons": "^1.5",
        "codeat3/blade-vaadin-icons": "^1.6",
        "davidhsianturi/blade-bootstrap-icons": "^2.1",
        "doctrine/dbal": "^4.3",
        "eduard9969/blade-polaris-icons": "^2.8",
        "filament/filament": "3.3",
        "joshembling/image-optimizer": "^1.6",
        "laravel/framework": "^12.0",
        "laravel/tinker": "^2.10.1",
        "mallardduck/blade-lucide-icons": "^1.23",
        "mansoor/blade-lets-icons": "^1.0",
        "mokhosh/filament-jalali": "^4.10",
        "morilog/jalali": "^3.4",
        "mpdf/mpdf": "^8.2",
        "owenvoke/blade-entypo": "^2.3",
        "owenvoke/blade-fontawesome": "^2.9",
        "postare/blade-mdi": "^1.1",
        "saade/blade-iconsax": "^1.2"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "phpunit/phpunit": "^11.5.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php artisan config:clear",
            "@php artisan clear-compiled",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
